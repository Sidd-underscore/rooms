 <!-- Firebase -->
  <script src="https://www.gstatic.com/firebasejs/7.13.2/firebase-app.js"></script>
  <script src="https://www.gstatic.com/firebasejs/7.13.2/firebase-auth.js"></script>
  <script src="https://www.gstatic.com/firebasejs/7.13.2/firebase-database.js"></script>
         <meta name="viewport" content="width=device-width, initial-scale=1" />

      <script>
         const ROOM_ID = "<%= roomId %>";
      </script>
     <style>

#messages{
    padding-bottom: 30%;
}

       li img {
             height: 40px;
    width: 40px;
    position: relative;
    top: 30px;
    margin-left: 0;
       }

       li p {
             margin-left: 60px;
    margin-top: 10px;
         
       }
li {
    list-style-type: none;
    margin-bottom: 10px;
    padding: 5px;
    border-radius: 10px;
    color: white;
    width: 50%;
}

li span {
    font-style: italic;
  padding: 5px;
  border-radius: 5px;
    color: white;
}
     </style>
   </head>
   <body style="height: 98vh; width: 100vw; background-color: transparent !important;" onunload="bye()">
      <div class="main__chat_window"  >
         <div class="messages">
         </div>
      </div>
      <div class="main__bottom">
         <div class="format">
         </div>
         <div class="main__message_container">
            <div class="stuff-container">
               <div class="stuff">
                  <div onclick="image()">
                     <span class="material-icons-outlined md-48">
                     file_upload
                     </span>
                     <br>
                     <p>Upload a File</p>
                  </div>
                  <div class="share-button" onclick="invite()">
                     <span class="material-icons-outlined md-48">
                     group_add</span>
                     <br>
                     <p>Invite</p>
                  </div>
                  <div onclick="afk()">
                     <span class="material-icons md-48">
                     ðŸ’¤ 
                     </span>            <br>
                     <p>Go AFK</p>
                  </div>
               </div>
            </div>
            <div id="chat_message"></div>
            <button id="send" onclick="sendMsg()" class="options__button">
               <i class="fa fa-send" aria-hidden="true"></i>
            </button>
         </div>
      </div>
      <div id="snackbar">Message sent!</div>
      <div id="settings-modal" class="modal">
         <!-- Modal content -->
         <div class="modal-content">
            <span class="close">&times;</span>
            <iframe src="../../settings"></iframe>
         </div>
      </div>
      </div>
      </div>
     <div class="share-dialog">
  <header>
    <h3 class="dialog-title">Share this pen</h3>
    <button class="close-button"><svg><use href="#close"></use></svg></button>
  </header>
  <div class="targets">

    
    <a class="button">
      <svg>
        <use href="#twitter"></use>
      </svg>
      <span>Twitter</span>
    </a>
    

    
    <a class="button">
      <svg>
        <use href="#email"></use>
      </svg>
      <span>Email</span>
    </a>
  </div>
         <div class="link">
    <div class="url"></div>
    <button class="copy-link">Copy Link</button>
  </div>
</div>
      <script src="https://cdnjs.cloudflare.com/ajax/libs/showdown/1.8.6/showdown.min.js"></script>
      <script src="/js/global.js"></script>
      <script src="/js/vex.min.js"></script>
      <script src="//static.filestackapi.com/filestack-js/3.x.x/filestack.min.js"></script>
      <script src="https://cdnjs.cloudflare.com/ajax/libs/axios/0.19.2/axios.min.js"></script>
     <script src="/js/group.js"></script>
      <script>vex.defaultOptions.className = 'vex-theme-os'</script>
      <script type="module" src="/js/firebase.js"></script>
     <!-- no important, but kinda -->
     <svg class="hidden">
  <defs>
    
    
    <symbol id="twitter" viewBox="0 0 24 24" fill="#1da1f2" stroke="#1da1f2" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-twitter"><path d="M23 3a10.9 10.9 0 0 1-3.14 1.53 4.48 4.48 0 0 0-7.86 3v1A10.66 10.66 0 0 1 3 4s-4 9 5 13a11.64 11.64 0 0 1-7 2c9 5 20 0 20-11.5a4.5 4.5 0 0 0-.08-.83A7.72 7.72 0 0 0 23 3z"></path></symbol>
    
    <symbol id="email" viewBox="0 0 24 24" fill="#777" stroke="#fafafa" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-mail"><path d="M4 4h16c1.1 0 2 .9 2 2v12c0 1.1-.9 2-2 2H4c-1.1 0-2-.9-2-2V6c0-1.1.9-2 2-2z"></path><polyline points="22,6 12,13 2,6"></polyline></symbol>
    
    
    <symbol id="close" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-x-square"><rect x="3" y="3" width="18" height="18" rx="2" ry="2"></rect><line x1="9" y1="9" x2="15" y2="15"></line><line x1="15" y1="9" x2="9" y2="15"></line></symbol>
  </defs>
</svg>
   </body>
</html>
