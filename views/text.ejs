<html lang="en">
   <head>
      <title id="title">Rooms | Text</title>
      <script src="https://kit.fontawesome.com/58c1e58602.js" crossorigin="anonymous"></script>
      <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
      <meta name="description" content="Rooms is a text and video chat app, for your everyday needs. Chill and get stuff done now with Rooms!" />
      <meta charset="utf-8" />
      <meta http-equiv="X-UA-Compatible" content="IE=edge" />
      <meta name="viewport" content="width=device-width, initial-scale=1" />
      <link rel="stylesheet" href="/css/style.css" />
      <link rel="stylesheet" href="/css/vex-theme-os.css" />
      <link rel="stylesheet" href="/css/vex.css" />
      <link rel="icon" type="image/x-icon" href="/img/favicon.ico">
      <script src="https://kit.fontawesome.com/04f078ab1a.js" crossorigin="anonymous"></script>
      <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet" />
      <link href="https://fonts.googleapis.com/icon?family=Material+Icons+Outlined" rel="stylesheet" />
      <meta charset="UTF-8">
      <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/9.12.0/styles/monokai-sublime.min.css">
      <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/KaTeX/0.7.1/katex.min.css">
      <script src="https://cdn.quilljs.com/1.3.6/quill.js"></script>
